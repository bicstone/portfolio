{
  "name": "bicstone.me",
  "private": true,
  "description": "Oishi Takanori Portfolio",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build --verbose",
    "develop": "gatsby develop --port 10014 --verbose",
    "dev": "yarn develop",
    "start": "yarn develop",
    "serve": "gatsby serve --port 20014",
    "clean": "gatsby clean",
    "lint": "eslint --cache --ignore-path .eslintignore --max-warnings=0 --ext .js,.ts,.tsx .",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --cache --ignore-path .eslintignore --write .",
    "extract": "esbuild i18next-extract.babel.config.ts --outfile=babel.config.js --bundle --platform=node && babel -o .tmp.js src/**/*.{js,jsx,ts,tsx} && rimraf babel.config.js .tmp.js",
    "t": "yarn extract",
    "g": "graphql-codegen",
    "typecheck": "tsc -p . --noEmit",
    "tc": "yarn typecheck",
    "deploy": "gatsby-plugin-s3 deploy",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "@mdx-js/mdx": "1.6.22",
