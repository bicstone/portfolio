{
  "name": "bicstone.me",
  "private": true,
  "description": "Takanori Oishi Portfolio",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "dev": "yarn develop",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint --max-warnings=0 --cache --ext .js,.ts,.tsx .",
    "lint:fix": "yarn lint --fix",
    "extract": "yarn run babel --config-file ./babel-extract.config.js -o tmp/chunk.js src/**/*.{js,jsx,ts,tsx} && rimraf tmp",
    "t": "yarn extract",
    "codegen": "graphql-codegen",
    "g": "yarn codegen",
    "typecheck": "tsc -p . --noEmit",
    "tc": "yarn typecheck",
    "barrelsby": "barrelsby -d ./src -D -i \"src/(components|contexts|hooks|reducers|types)/(?!.*\\.d\\.ts$).*$\" -l below -q",
    "b": "yarn barrelsby",
    "prepare": "husky install"
  },
  "dependencies": {
    "@material-ui/core": "4.12.2",
    "@material-ui/icons": "4.11.2",
    "@material-ui/lab": "4.0.0-alpha.60",
