{
  "name": "masshiro.me",
  "private": true,
  "description": "Takanori Oishi Portfolio",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "dev": "yarn develop",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint --max-warnings=0 --cache --ext .js,.ts,.tsx .",
    "lint:fix": "yarn lint --fix",
    "extract": "yarn run babel --config-file ./babel-extract.config.js -o tmp/chunk.js src/**/*.{js,jsx,ts,tsx} && rimraf tmp",
    "t": "yarn extract",
    "codegen": "graphql-codegen",
    "g": "yarn codegen",
    "typecheck": "tsc -p . --noEmit",
    "tc": "yarn typecheck",
    "barrelsby": "barrelsby -d ./src -D -i \"src/(components|hooks|reducers|types)/\" -l below -q",
    "b": "yarn barrelsby"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/styles": "^4.10.0",
    "clsx": "^1.1.1",
    "gatsby": "^2.26.1",
    "gatsby-image": "^2.5.0",
    "gatsby-plugin-advanced-sitemap": "^1.5.6",
    "gatsby-plugin-google-analytics": "^2.5.0",
    "gatsby-plugin-manifest": "^2.6.1",
    "gatsby-plugin-netlify": "^2.5.0",
    "gatsby-plugin-netlify-cache": "^1.2.0",
    "gatsby-plugin-offline": "^3.4.0",
    "gatsby-plugin-react-i18next": "^0.0.27",
    "gatsby-plugin-root-import": "^2.0.5",
    "gatsby-plugin-sharp": "^2.8.0",
    "gatsby-source-contentful": "^4.0.0",
    "gatsby-source-filesystem": "^2.5.0",
    "gatsby-theme-material-ui": "^1.0.13",
    "gatsby-transformer-inline-svg": "^0.0.7",
    "gatsby-transformer-sharp": "^2.6.0",
    "html-react-parser": "^0.14.1",
    "i18next": "^19.8.3",
    "js-cookie": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.7.3",
    "typed.js": "^2.0.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/plugin-transform-typescript": "^7.12.1",
    "@graphql-codegen/cli": "^1.19.1",
    "@graphql-codegen/typescript": "^1.17.11",
    "@graphql-codegen/typescript-operations": "^1.17.9",
    "@types/js-cookie": "^2.2.6",
    "@types/node": "^14.14.7",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/react-helmet": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "babel-plugin-i18next-extract": "^0.8.2",
    "babel-preset-gatsby": "^0.6.0",
    "barrelsby": "^2.2.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": "^16.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-standard": "^4.1.0",
    "husky": "^4.3.0",
    "prettier": "2.1.2",
    "typescript": "^4.0.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bicstone/masshiro.me"
  },
  "bugs": {
    "url": "https://github.com/bicstone/masshiro.me/issues"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn barrelsby && yarn tc && yarn lint"
    }
  }
}
