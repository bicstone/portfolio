{
  "name": "bicstone.me",
  "private": true,
  "description": "Takanori Oishi Portfolio",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "dev": "yarn develop",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint --max-warnings=0 --cache --ext .js,.ts,.tsx .",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write .",
    "extract": "yarn run babel --config-file ./i18next-extract.babel.config -o tmp/chunk.js src/**/*.{js,jsx,ts,tsx} && rimraf tmp",
    "t": "yarn extract",
    "codegen": "graphql-codegen",
    "g": "yarn codegen",
    "typecheck": "tsc -p . --noEmit",
    "tc": "yarn typecheck",
    "barrelsby": "barrelsby -d ./src -D -i \"src/(components|contexts|hooks|reducers|types)/(?!.*\\.d\\.ts$).*$\" -l below -q",
    "b": "yarn barrelsby",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "11.7.0",
    "@emotion/styled": "11.6.0",
    "@mui/icons-material": "5.2.1",
    "@mui/lab": "5.0.0-alpha.59",
    "@mui/material": "5.2.3",
    "gatsby": "4.3.0",
    "gatsby-plugin-advanced-sitemap": "2.0.0",
    "gatsby-plugin-csp": "1.1.3",
    "gatsby-plugin-emotion": "7.3.0",
    "gatsby-plugin-google-analytics": "4.3.0",
    "gatsby-plugin-image": "2.3.0",
    "gatsby-plugin-manifest": "4.3.0",
    "gatsby-plugin-react-i18next": "1.2.2",
    "gatsby-plugin-remove-serviceworker": "1.0.0",
    "gatsby-plugin-root-import": "2.0.8",
    "gatsby-plugin-sharp": "4.3.0",
    "gatsby-source-contentful": "7.1.0",
    "gatsby-source-filesystem": "4.3.0",
    "gatsby-transformer-inline-svg": "1.0.0",
    "gatsby-transformer-remark": "5.3.0",
    "gatsby-transformer-sharp": "4.3.0",
    "html-react-parser": "1.4.2",
    "i18next": "21.6.0",
    "js-cookie": "3.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-i18next": "11.15.0"
  },
  "devDependencies": {
    "@babel/cli": "7.16.0",
    "@babel/plugin-transform-typescript": "7.16.1",
    "@graphql-codegen/cli": "2.3.0",
    "@graphql-codegen/typescript": "2.4.1",
    "@graphql-codegen/typescript-operations": "2.2.1",
    "@types/js-cookie": "3.0.1",
    "@types/node": "16.11.12",
    "@types/react": "17.0.37",
    "@types/react-dom": "17.0.11",
    "@types/react-helmet": "6.1.4",
    "@typescript-eslint/eslint-plugin": "5.6.0",
    "@typescript-eslint/parser": "5.6.0",
    "babel-plugin-i18next-extract": "0.8.3",
    "babel-preset-gatsby": "2.3.0",
    "barrelsby": "2.3.0",
    "dotenv": "10.0.0",
    "eslint": "8.4.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.2.0",
