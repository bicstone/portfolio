{
  "name": "bicstone.me",
  "private": true,
  "description": "Oishi Takanori Portfolio",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop --port 10014",
    "serve": "gatsby serve --port 20014",
    "clean": "gatsby clean",
    "lint": "yarn lint:src && yarn lint:md && yarn lint:text",
    "lint:fix": "yarn lint:src --fix && lint:md --fix && yarn lint:text --fix",
    "lint:src": "eslint --cache --ignore-path .eslintignore --max-warnings=0 --ext .js,.ts,.tsx .",
    "lint:md": "cd content && yarn lint:md",
    "lint:text": "cd content && yarn lint:text",
    "format": "prettier --cache --ignore-path .eslintignore --write .",
    "extract": "esbuild i18next-extract.babel.config.ts --outfile=babel.config.js --bundle --platform=node && babel -o .tmp.js src/**/*.{js,jsx,ts,tsx} && rimraf babel.config.js .tmp.js",
    "t": "yarn extract",
    "g": "graphql-codegen",
    "typecheck": "tsc -p . --noEmit",
    "deploy": "gatsby-plugin-s3 deploy",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "@mdx-js/mdx": "2.1.3",
    "@mdx-js/react": "2.1.3",
    "@mui/icons-material": "5.10.6",
    "@mui/material": "5.10.8",
    "@sentry/gatsby": "7.14.2",
    "date-fns": "2.29.3",
    "date-fns-tz": "1.3.7",
    "fuse.js": "6.6.2",
    "gatsby": "4.24.3",
    "gatsby-plugin-emotion": "7.24.0",
    "gatsby-plugin-fix-fouc": "0.1.2",
    "gatsby-plugin-google-tagmanager": "4.24.0",
    "gatsby-plugin-image": "2.24.0",
    "gatsby-plugin-layout": "3.24.0",
    "gatsby-plugin-mdx": "4.3.0",
    "gatsby-plugin-react-i18next": "2.0.4",
    "gatsby-plugin-s3": "0.3.8",
    "gatsby-plugin-sharp": "4.24.0",
    "gatsby-plugin-sitemap": "5.24.0",
    "gatsby-remark-autolink-headers": "5.24.0",
    "gatsby-remark-prismjs": "6.24.0",
    "gatsby-source-contentful": "7.22.0",
    "gatsby-source-filesystem": "4.24.0",
    "gatsby-transformer-sharp": "4.24.0",
    "html-react-parser": "3.0.4",
    "i18next": "21.10.0",
    "prismjs": "1.29.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hotkeys-hook": "3.4.7",
    "react-i18next": "11.18.6"
  },
  "devDependencies": {
    "@babel/cli": "7.19.3",
    "@babel/plugin-transform-typescript": "7.19.3",
    "@graphql-codegen/cli": "2.13.5",
    "@graphql-codegen/introspection": "2.2.1",
    "@graphql-codegen/typescript": "2.7.3",
    "@graphql-codegen/typescript-operations": "2.5.3",
    "@types/babel__core": "7.1.19",
    "@types/node": "16.11.64",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.39.0",
    "@typescript-eslint/parser": "5.39.0",
    "babel-plugin-i18next-extract": "0.9.0",
    "babel-preset-gatsby": "2.24.0",
    "dotenv": "16.0.3",
    "esbuild": "0.15.10",
    "eslint": "8.25.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard-with-typescript": "23.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-n": "15.3.0",
    "eslint-plugin-promise": "6.0.1",
    "eslint-plugin-react": "7.31.8",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-regexp": "1.9.0",
    "husky": "8.0.1",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "typescript": "4.8.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bicstone/portfolio"
  },
  "bugs": {
    "url": "https://github.com/bicstone/portfolio/issues"
  },
  "browserslist": [
    "last 3 Chrome major versions",
    "last 3 Firefox major versions",
    "last 3 Edge major versions",
    "last 3 Safari major versions",
    "last 3 Android major versions",
    "last 3 ChromeAndroid major versions",
    "last 3 iOS major versions"
  ],
  "packageManager": "yarn@3.2.4",
  "workspaces": [
    "content"
  ]
}
