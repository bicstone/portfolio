{
  "name": "bicstone.me",
  "private": true,
  "description": "Oishi Takanori Portfolio",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "yarn build:vanilla && yarn build:gatsby",
    "build:gatsby": "gatsby build --verbose",
    "build:vanilla": "esbuild src/vanilla/index.ts --outdir=static/vanilla --bundle --minify --target=es2015,chrome97,firefox96,safari14,edge97,ios14",
    "develop": "yarn build:vanilla && gatsby develop --port 10014 --verbose",
    "dev": "yarn develop",
    "start": "yarn develop",
    "serve": "gatsby serve --port 20014",
    "clean": "gatsby clean",
    "lint": "eslint --cache --ignore-path .eslintignore --max-warnings=0 --ext .js,.ts,.tsx .",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --cache --ignore-path .eslintignore --write .",
    "extract": "esbuild i18next-extract.babel.config.ts --outfile=babel.config.js --bundle --platform=node && babel -o .tmp.js src/**/*.{js,jsx,ts,tsx} && rimraf babel.config.js .tmp.js",
    "t": "yarn extract",
    "g": "graphql-codegen",
    "typecheck": "tsc -p . --noEmit",
    "tc": "yarn typecheck",
    "barrelsby": "barrelsby -d ./src -D -i 'src/(components|contexts|hooks|reducers|types)/(?!.*\\.d\\.ts$).*$' -l below -q",
    "b": "yarn barrelsby",
    "deploy": "gatsby-plugin-s3 deploy",
    "prepare": "husky install && yarn build:vanilla"
  },
  "dependencies": {
    "@emotion/react": "11.10.0",
    "@emotion/styled": "11.10.0",
    "@mdx-js/mdx": "1.6.22",
    "@mdx-js/react": "1.6.22",
