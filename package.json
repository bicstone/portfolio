{
  "name": "bicstone.me",
  "private": true,
  "description": "Oishi Takanori Portfolio",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop --port 10014",
    "dev": "yarn develop",
    "start": "yarn develop",
    "serve": "gatsby serve --port 20014",
    "clean": "gatsby clean",
    "lint": "eslint --max-warnings=0 --cache --ext .js,.ts,.tsx .",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write .",
    "extract": "yarn run babel --config-file ./i18next-extract.babel.config -o tmp/chunk.js src/**/*.{js,jsx,ts,tsx} && rimraf tmp",
    "t": "yarn extract",
    "graphql-code-generate": "graphql-codegen --config graphql-codegen-config.json",
    "g": "yarn graphql-code-generate",
    "typecheck": "tsc -p . --noEmit",
    "tc": "yarn typecheck",
    "barrelsby": "barrelsby -d ./src -D -i 'src/(components|contexts|hooks|reducers|types)/(?!.*\\.d\\.ts$).*$' -l below -q",
    "b": "yarn barrelsby",
    "prepare": "husky install",
    "deploy": "gatsby-plugin-s3 deploy"
  },
  "dependencies": {
    "@emotion/react": "11.8.2",
