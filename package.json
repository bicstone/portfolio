{
  "name": "bicstone.me",
  "private": true,
  "description": "Takanori Oishi Portfolio",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "dev": "yarn develop",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint --max-warnings=0 --cache --ext .js,.ts,.tsx .",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write .",
    "extract": "yarn run babel --config-file ./babel-extract.config.js -o tmp/chunk.js src/**/*.{js,jsx,ts,tsx} && rimraf tmp",
    "t": "yarn extract",
    "codegen": "graphql-codegen",
    "g": "yarn codegen",
    "typecheck": "tsc -p . --noEmit",
    "tc": "yarn typecheck",
    "barrelsby": "barrelsby -d ./src -D -i \"src/(components|contexts|hooks|reducers|types)/(?!.*\\.d\\.ts$).*$\" -l below -q",
    "b": "yarn barrelsby",
    "prepare": "husky install"
  },
  "dependencies": {
    "@material-ui/core": "4.12.3",
    "@material-ui/icons": "4.11.2",
    "@material-ui/lab": "4.0.0-alpha.60",
    "@material-ui/styles": "4.11.4",
    "clsx": "1.1.1",
    "gatsby": "3.12.1",
    "gatsby-image": "3.11.0",
    "gatsby-plugin-advanced-sitemap": "2.0.0",
    "gatsby-plugin-csp": "1.1.3",
    "gatsby-plugin-google-analytics": "3.12.0",
    "gatsby-plugin-image": "1.12.0",
    "gatsby-plugin-manifest": "3.12.0",
    "gatsby-plugin-offline": "4.12.0",
    "gatsby-plugin-react-i18next": "1.1.1",
    "gatsby-plugin-root-import": "2.0.7",
    "gatsby-plugin-sharp": "3.12.0",
    "gatsby-source-contentful": "5.12.0",
    "gatsby-source-filesystem": "3.12.0",
    "gatsby-theme-material-ui": "2.0.1",
    "gatsby-transformer-inline-svg": "0.1.0",
    "gatsby-transformer-remark": "4.9.0",
    "gatsby-transformer-sharp": "3.12.0",
    "html-react-parser": "1.2.8",
    "i18next": "20.4.0",
    "js-cookie": "3.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-i18next": "11.11.4"
  },
  "devDependencies": {
    "@babel/cli": "7.14.8",
    "@babel/plugin-transform-typescript": "7.15.0",
    "@graphql-codegen/cli": "2.1.1",
    "@graphql-codegen/typescript": "2.1.2",
    "@graphql-codegen/typescript-operations": "2.1.2",
    "@types/js-cookie": "2.2.7",
    "@types/node": "14.17.12",
    "@types/react": "17.0.19",
    "@types/react-dom": "17.0.9",
    "@types/react-helmet": "6.1.2",
    "@typescript-eslint/eslint-plugin": "4.30.0",
    "@typescript-eslint/parser": "4.30.0",
    "babel-plugin-i18next-extract": "0.8.3",
    "babel-preset-gatsby": "1.12.0",
    "barrelsby": "2.2.0",
    "dotenv": "10.0.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-react": "7.25.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-standard": "4.1.0",
    "husky": "7.0.2",
    "null-loader": "4.0.1",
    "prettier": "2.3.2",
    "typescript": "4.4.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bicstone/portfolio"
  },
  "bugs": {
    "url": "https://github.com/bicstone/portfolio/issues"
  },
  "engines": {
    "node": ">=12.13.0"
  }
}
