    "gatsby": "3.10.1",
    "gatsby-image": "3.10.0",
    "gatsby-plugin-advanced-sitemap": "2.0.0",
    "gatsby-plugin-csp": "1.1.3",
    "gatsby-plugin-google-analytics": "3.10.0",
    "gatsby-plugin-image": "1.10.1",
    "gatsby-plugin-manifest": "3.10.0",
    "gatsby-plugin-offline": "4.10.0",
    "gatsby-plugin-react-i18next": "1.1.1",
    "gatsby-plugin-root-import": "2.0.6",
    "gatsby-plugin-sharp": "3.10.2",
    "gatsby-plugin-ts-config": "1.1.5",
    "gatsby-source-contentful": "5.10.0",
    "gatsby-source-filesystem": "3.10.0",
    "gatsby-theme-material-ui": "2.0.1",
    "gatsby-transformer-inline-svg": "0.1.0",
    "gatsby-transformer-remark": "4.7.0",
    "gatsby-transformer-sharp": "3.10.0",
    "html-react-parser": "1.2.7",
    "i18next": "20.3.4",
    "js-cookie": "2.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-i18next": "11.11.3"
