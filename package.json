    "gatsby-plugin-manifest": "4.11.1",
    "gatsby-plugin-mdx": "3.11.1",
    "gatsby-plugin-mdx-embed": "1.0.0",
    "gatsby-plugin-next-seo": "1.10.0",
    "gatsby-plugin-react-i18next": "1.2.2",
    "gatsby-plugin-remove-serviceworker": "1.0.0",
    "gatsby-plugin-root-import": "2.0.8",
    "gatsby-plugin-s3": "0.3.8",
    "gatsby-plugin-sharp": "4.11.1",
    "gatsby-remark-images-contentful": "5.11.0",
