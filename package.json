    "@mui/icons-material": "5.10.6",
    "@mui/material": "5.10.6",
    "@sentry/gatsby": "7.13.0",
    "fuse.js": "6.6.2",
    "gatsby": "4.23.1",
    "gatsby-plugin-emotion": "7.23.0",
    "gatsby-plugin-fix-fouc": "0.1.2",
    "gatsby-plugin-google-tagmanager": "4.23.1",
    "gatsby-plugin-image": "2.23.1",
    "gatsby-plugin-mdx": "3.20.0",
    "gatsby-plugin-mdx-embed": "1.0.0",
    "gatsby-plugin-next-seo": "1.10.0",
    "gatsby-plugin-react-i18next": "2.0.4",
    "gatsby-plugin-s3": "0.3.8",
    "gatsby-plugin-sharp": "4.23.1",
    "gatsby-plugin-sitemap": "5.23.1",
    "gatsby-remark-autolink-headers": "5.23.1",
    "gatsby-remark-images-contentful": "5.23.0",
    "gatsby-remark-prismjs": "6.23.0",
    "gatsby-source-contentful": "7.21.1",
    "gatsby-source-filesystem": "4.23.0",
    "gatsby-transformer-inline-svg": "1.2.0",
    "gatsby-transformer-sharp": "4.23.1",
    "html-react-parser": "3.0.4",
    "i18next": "21.9.2",
    "mdx-embed": "1.0.0",
    "prism-themes": "1.9.0",
    "prismjs": "1.29.0",
    "react": "18.2.0",
