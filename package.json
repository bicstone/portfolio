{
  "name": "bicstone.me",
  "private": true,
  "description": "Takanori Oishi Portfolio",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "dev": "yarn develop",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint --max-warnings=0 --cache --ext .js,.ts,.tsx .",
    "lint:fix": "yarn lint --fix",
    "extract": "yarn run babel --config-file ./babel-extract.config.js -o tmp/chunk.js src/**/*.{js,jsx,ts,tsx} && rimraf tmp",
    "t": "yarn extract",
    "codegen": "graphql-codegen",
    "g": "yarn codegen",
    "typecheck": "tsc -p . --noEmit",
    "tc": "yarn typecheck",
    "barrelsby": "barrelsby -d ./src -D -i \"src/(components|hooks|reducers|types)/\" -l below -q",
    "b": "yarn barrelsby"
  },
  "dependencies": {
    "@material-ui/core": "4.11.2",
    "@material-ui/icons": "4.11.2",
    "@material-ui/lab": "4.0.0-alpha.57",
    "@material-ui/styles": "4.11.2",
    "clsx": "1.1.1",
    "gatsby": "2.29.3",
    "gatsby-image": "2.8.0",
    "gatsby-plugin-advanced-sitemap": "1.5.6",
    "gatsby-plugin-csp": "1.1.3",
    "gatsby-plugin-google-analytics": "2.8.0",
    "gatsby-plugin-manifest": "2.9.1",
    "gatsby-plugin-offline": "3.7.1",
    "gatsby-plugin-react-i18next": "0.0.27",
    "gatsby-plugin-root-import": "2.0.5",
    "gatsby-plugin-sharp": "2.11.2",
    "gatsby-plugin-ts-config": "1.1.5",
    "gatsby-source-contentful": "4.3.1",
    "gatsby-source-filesystem": "2.8.1",
    "gatsby-theme-material-ui": "1.0.13",
    "gatsby-transformer-inline-svg": "0.0.7",
    "gatsby-transformer-remark": "2.13.1",
    "gatsby-transformer-sharp": "2.9.0",
    "html-react-parser": "1.1.1",
    "i18next": "19.8.4",
    "js-cookie": "2.2.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "react-i18next": "11.8.5"
  },
  "devDependencies": {
    "@babel/cli": "7.12.10",
    "@babel/plugin-transform-typescript": "7.12.1",
    "@graphql-codegen/cli": "1.20.0",
    "@graphql-codegen/typescript": "1.20.0",
    "@graphql-codegen/typescript-operations": "1.17.13",
    "@types/js-cookie": "2.2.6",
    "@types/node": "14.14.17",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-helmet": "6.1.0",
    "@typescript-eslint/eslint-plugin": "4.11.1",
    "@typescript-eslint/parser": "4.11.1",
    "babel-plugin-i18next-extract": "0.8.2",
    "babel-preset-gatsby": "0.9.1",
    "barrelsby": "2.2.0",
    "dotenv": "8.2.0",
    "eslint": "7.16.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-config-standard": "16.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.3.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-standard": "4.1.0",
    "husky": "4.3.6",
    "prettier": "2.2.1",
    "typescript": "4.1.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bicstone/portfolio"
  },
  "bugs": {
    "url": "https://github.com/bicstone/portfolio/issues"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn barrelsby && yarn typecheck && yarn lint"
    }
  }
}
