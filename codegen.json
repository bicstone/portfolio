{
  "overwrite": true,
  "schema": "http://localhost:8000/___graphql",
  "documents": ["./src/**/*.{ts,tsx}"],
  "generates": {
    "src/types/graphqlTypes.ts": {
      "plugins": ["typescript", "typescript-operations"]
    }
  },
  "config": {
    "namingConvention": {
      "enumValues": "keep"
    }
  }
}
