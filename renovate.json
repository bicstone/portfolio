{
  "extends": ["config:base"],
  "timezone": "Asia/Tokyo",
  "schedule": ["before 6am on Sunday"],
  "assignees": ["bicstone"],
  "reviewers": ["bicstone"],
  "packageRules": [
    {
      "matchPackagePatterns": ["*"],
      "groupName": "dependencies"
    },
    {
      "matchPackagePatterns": ["eslint"],
      "groupName": "eslint dependencies"
    },
    {
      "matchPackagePatterns": ["gatsby"],
      "groupName": "gatsby dependencies"
    },
    {
      "matchPackagePatterns": ["material-ui"],
      "groupName": "mui dependencies"
    },
    {
      "matchPackagePrefixes": ["@mui"],
      "groupName": "mui dependencies"
    },
    {
      "matchPackagePrefixes": ["@emotion"],
      "groupName": "emotion dependencies"
    }
  ]
}
